<My_include>

    <prototype>
        <name>enrichment</name>
        <config>
            <Enrichment>
                <feed_commod>c_natl_u</feed_commod>
                <feed_recipe>r_natl_u</feed_recipe>
                <product_commod>c_uox</product_commod>
                <tails_commod>c_du</tails_commod>


                <tails_assay>0.0025</tails_assay>
                <swu_capacity>1e100</swu_capacity>
                <initial_feed>1e100</initial_feed>
            </Enrichment>
        </config>
    </prototype>

    <prototype>
        <name>separation</name>
        <config>
            <Separations>
                <leftover_commod>c_waste</leftover_commod>
                <!--
                <throughput>60000</throughput>
                <feedbuf_size>66000</feedbuf_size>
                -->
                <throughput>1e100</throughput>
                <feedbuf_size>1e100</feedbuf_size>
                <feed_commods>
                    <val>c_uox_a_sc_str</val>
                    <val>c_uox_b_sc_str</val>
                    <val>c_sfr_a_sc_str</val>
                    <val>c_sfr_b_sc_str</val>
                </feed_commods>
                <feed_commod_prefs>
                    <val>3.0</val>
                    <val>3.0</val>
                    <val>2.0</val>
                    <val>1.0</val>
                </feed_commod_prefs>
                <streams>
                    <item>
                        <commod>c_pu</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>.99</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>c_ru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>U</comp> <eff>.99</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>

            </Separations>
        </config>
    </prototype>

    <prototype>
        <name>fab_sfr_a</name>
        <!--        <lifetime>960</lifetime>-->
        <config>
            <FuelFab>
                <fill_commods>
                    <val>c_ru_str</val>
                    <val>c_du_str</val>
                    <val>c_du_src</val>
                </fill_commods>
                <fill_commod_prefs>
                    <val>100</val>
                    <val>10</val>
                    <val>1</val>
                </fill_commod_prefs>
                <fill_recipe>r_du</fill_recipe>
                <fill_size>186000</fill_size>

                <fiss_commods>
                    <val>c_pu_str</val>
                </fiss_commods>
                <fiss_size>18000</fiss_size>

                <throughput>2000000</throughput>
                <spectrum>fission_spectrum_ave</spectrum>
                <outcommod>c_sfr_fuel</outcommod>
            </FuelFab>
        </config>
    </prototype>

    <prototype>
        <name>fab_sfr_b</name>
        <lifetime>960</lifetime>
        <config>
            <FuelFab>
                <fill_commods>
                    <val>c_ru_str</val>
                    <val>c_du_str</val>
                    <val>c_du_src</val>
                </fill_commods>
                <fill_commod_prefs>
                    <val>100</val>
                    <val>10</val>
                    <val>1</val>
                </fill_commod_prefs>
                <fill_recipe>r_du</fill_recipe>
                <fill_size>1e100</fill_size>

                <fiss_commods>
                    <val>c_pu_str</val>
                </fiss_commods>
                <fiss_size>1e100</fiss_size>

                <throughput>1e100</throughput>
                <spectrum>fission_spectrum_ave</spectrum>
                <outcommod>c_sfr_fuel</outcommod>
            </FuelFab>
        </config>
    </prototype>
    
</My_include>
