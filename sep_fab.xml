<My_include>

    <prototype>
        <name>enrichment</name>
        <config>
            <Enrichment>
                <feed_commod>c_natl_u</feed_commod>
                <feed_recipe>r_natl_u</feed_recipe>
                <product_commod>c_uox</product_commod>
                <tails_commod>c_du</tails_commod>


                <tails_assay>0.0025</tails_assay>
                <swu_capacity>1e100</swu_capacity>
                <initial_feed>1e100</initial_feed>
            </Enrichment>
        </config>
    </prototype>

    <prototype>
        <name>separation</name>
        <config>
            <Separations>
                <leftover_commod>c_waste</leftover_commod>
                <throughput>60000</throughput>
                <feedbuf_size>66000</feedbuf_size>
                <feed_commods>
                    <val>UOX_A_spent_stored</val>
                    <val>UOX_B_spent_stored</val>
                    <val>SFR_A_spent_stored</val>
                    <val>SFR_B_spent_stored</val>
                </feed_commods>
                <feed_commod_prefs>
                    <val>3.0</val>
                    <val>3.0</val>
                    <val>2.0</val>
                    <val>1.0</val>
                </feed_commod_prefs>
                <streams>
                    <item>
                        <commod>c_pu</commod>
                        <info>
                            <buf_size>6000</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>.99</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>c_ru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>U</comp> <eff>.99</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>

            </Separations>
        </config>
    </prototype>

    <prototype>
        <name>SFR_FAB_A</name>
        <lifetime>960</lifetime>
        <config>
            <FuelFab>
                <fill_commods><val>c_du</val></fill_commods>
                <fill_recipe>r_du</fill_recipe>
                <fill_size>69492</fill_size>

                <fiss_commods>
                    <val>pu</val>
                </fiss_commods>
                <fiss_size>5748</fiss_size>

                <throughput>75240</throughput>
                <spectrum>fission_spectrum_ave</spectrum>
                <outcommod>SFR_fuel</outcommod>
            </FuelFab>
        </config>
    </prototype>

    <prototype>
        <name>SFR_FAB_B</name>
        <lifetime>960</lifetime>
        <config>
            <FuelFab>
                <fill_commods><val>c_du</val></fill_commods>
                <fill_recipe>r_du</fill_recipe>
                <fill_size>69912</fill_size>

                <fiss_commods>
                    <val>pu</val>
                </fiss_commods>
                <fiss_size>5856</fiss_size>

                <throughput>76440</throughput>
                <spectrum>fission_spectrum_ave</spectrum>
                <outcommod>SFR_fuel</outcommod>
            </FuelFab>
        </config>
    </prototype>

</My_include>
