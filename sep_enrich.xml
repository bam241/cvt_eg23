                    <val>1.0</val>
                </feed_commod_prefs>
                <streams>
                    <item>
                        <commod>c_pu</commod>
                        <info>
                            <buf_size>5000</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>.99</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>c_ru</commod>
